{
  "manifest_version": 2,
  "name": "Funix - NowLive",
  "description": "L'extention qui vous permet de savoir quand FunixGaming est en live",
  "version": "1.1",
  "permissions": ["notifications"],
  "browser_action": {
    "default_popup": "page/index.html"
  },
  "icons": {
    "64" : "img/logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}